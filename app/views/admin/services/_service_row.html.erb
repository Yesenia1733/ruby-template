<tr id="service_row_<%= service.id %>">
    <td><%= service.id %></td>
    <td><%= service.name%></td>
    <td><%= service.amount%></td>
    <td>
        <%= link_to "Edit", edit_admin_service_path(service), data: {bs_toggle: "modal", bs_target: "#editModal", remote: true}, class: "btn btn-success" %> 
        <%= link_to "Delete", admin_service_path(service), method: :delete, data: {confirm: "Estas Seguro de Eliminarlo?", remote: true}, class: "btn btn-danger" %>
    </td>
</tr>